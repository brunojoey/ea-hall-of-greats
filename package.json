  },
  "homepage": "https://hall-of-greats.herokuapp.com/",
  "dependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.25",
    "nodemon": "^2.0.4",
    "path": "^0.12.7"
  },
